{
  "manifest_version": 3,
  "name": "Activity Tracker Extension",
  "version": "1.0",
  "permissions": ["scripting", "webNavigation", "tabs", "activeTab"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://localhost:3000/*"],
      "js": ["sourceContent.js"]
    }
  ]
}
